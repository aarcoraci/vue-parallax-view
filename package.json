{
  "name": "vue-parallax-view",
  "version": "0.2.2",
  "private": false,
  "author": "Angel Arcoraci",
  "main": "./dist/vue-parallax-view.common.js",
  "license": "MIT",
  "homepage": "https://github.com/aarcoraci/vue-parallax-view#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarcoraci/vue-parallax-view.git"
  },
  "keywords": [
    "parallax",
    "vue"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
